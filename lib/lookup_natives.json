{
  "ws": {
    "flaky": "win32",
    "test-command": {
      "win32": "node_modules\\\\.bin\\\\mocha -t 5000 -s 2400 test\\\\*.test.js"
    },
    "replace": true,
    "verify-node-gyp-called": true
  },
  "bufferutil": {
    "replace": true,
    "verify-node-gyp-called": true
  },
  "utf-8-validate": {
    "replace": true,
    "verify-node-gyp-called": true
  },
  "node-sass": [ {
      "test-name": "using node-gyp",
      "flaky": "win32",
      "env": { "SKIP_SASS_BINARY_DOWNLOAD_FOR_CI": 1 }
    }, {
      "test-name": "not using node-gyp",
      "flaky": "win32"
    }
  ],
  "kerberos": {
    "test-command": " ",
    "verify-node-gyp-called": true
  },
  "contextify": {
      "flaky": "win32",
      "prefix": "v",
      "install": ["--build-from-source"],
      "replace": true,
      "verify-node-gyp-called": true
  },
  "thread-sleep": {
      "replace": true,
      "install": ["--build-from-source"],
      "verify-node-gyp-called": true      
  },
  "weak": {
    "replace": true,
    "verify-node-gyp-called": true
  },
  "bcrypt": {
    "prefix": "v",
    "replace": true,
    "test-command": {
      "default": "./node_modules/.bin/nodeunit test",
      "win32": "node_modules\\\\.bin\\\\nodeunit test"
    },
    "verify-node-gyp-called": true
  },
  "hiredis": {
    "verify-node-gyp-called": true
  },
  "sqlite3": [ {
      "test-name": "using node-gyp",
      "master": true,
      "prefix": "v",
      "replace": true,
      "install": ["--build-from-source"],
      "verify-node-gyp-called": true
    }, {
      "test-name": "not using node-gyp",
      "flaky": "win32",
      "prefix": "v",
      "replace": true,
      "verify-node-gyp-not-called": true
    }
  ],
  "iconv": {
    "flaky": "win32",
    "prefix": "v",
    "replace": true,
    "verify-node-gyp-called": true
  },
  "ref": {
    "replace": true,
    "verify-node-gyp-called": true
  },
  "v8-profiler":  {
      "flaky": "win32",
      "node-version": ">=6.0.0",
      "prefix": "v",
      "replace": true,
      "install": ["--build-from-source"],
      "verify-node-gyp-called": true
  },
  "v8-debug": {
      "flaky": "win32",
      "node-version": ">=6.0.0",
      "prefix": "v",
      "replace": true,
      "install": ["--build-from-source"],
      "verify-node-gyp-called": true
  }
}
